<launch> 
    <arg name="config_file" default="$(find omnicar_ros_driver)/config/raspicam.yaml"/>

    <rosparam command="load" file="$(find omnicar_ros_driver)/config/omnicar.yaml" />  
    
    <node name="omnicar_driver_node" pkg="omnicar_ros_driver" type="omnicar_driver" output="screen"></node>
    <node name="range_sensor_node" pkg="omnicar_ros_driver" type="range_sensor_node.py" output="screen"></node>

    <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
      <rosparam command="load" file="$(arg config_file)"/>
    </node>

</launch>